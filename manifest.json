{
	"manifest_version": 2,
	"name": "Bionize",
	"version": "1.0.0",
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content.js", "apiUtil.js"]
		}
	],
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
		"default_title": "Bionize"
	},
	"permissions": ["activeTab"]
}
